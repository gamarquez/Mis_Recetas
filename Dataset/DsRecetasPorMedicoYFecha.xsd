                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        r.Nro_Receta, p.Nombre_Paciente + ' ' + p.Apellido_Paciente AS Expr1, p.Nro_Documento, m.Nombre_Medico + ' ' + m.Apellido_Medico AS Expr2, FORMAT(r.Fecha_Usuario_Entrega_A_Paciente, 'dd/MM/yyyy') AS Expr3, 
                         e.Descripcion
